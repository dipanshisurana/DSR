Code 1


install.packages("visualize")
library(visualize)
zvalue=(11-10.2)/(21/sqrt(55))
zvalue
pnorm(zvalue)
dnorm(zvalue)
pnorm(2.825218) - pnorm(-2.825218)
qnorm(0.05)
visualize.norm(stat=zvalue,mu=0,sd=1,section="upper")
visualize.norm(stat=c(2.825218,-2.825218),mu=0,sd=1,section="tails")

