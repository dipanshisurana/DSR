code 2



library(visualize)
zvalue=(152-152)/(2/sqrt(100))
zvalue
qnorm(0.95)
qnorm(0.95)
visualize.norm(stat=zvalue,mu=0,sd=1,section="upper")
install.packages("BSDA")
library(BSDA)
z.test(x=D$Machine.1,alternative="greater", sigma.x=2, , mu=50)
